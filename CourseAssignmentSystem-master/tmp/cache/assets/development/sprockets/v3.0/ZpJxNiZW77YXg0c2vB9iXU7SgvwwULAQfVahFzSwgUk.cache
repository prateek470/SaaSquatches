I"C(function() {
  $(function() {
    $(document).on('change', "#faculty_id", function(evt) {
      return $.ajax('course_assignments/update_faculty_details', {
        type: 'GET',
        dataType: 'script',
        data: {
          faculty_id: $("#faculty_id option:selected").val()
        }
      });
    });
    return $(document).on('click', '#show_faculty_pref', function(evt) {
      return $.ajax('course_assignments/show_faculty_preference', {
        type: 'GET',
        dataType: 'script',
        data: {
          faculty_id: $("#faculty_id option:selected").val()
        },
        success: function(data, textStatus, jqXHR) {
          return alert('Success');
        },
        error: function(xhr) {
          return alert('Some error in Show Faculty Preference');
        }
      });
    });
  });

}).call(this);
